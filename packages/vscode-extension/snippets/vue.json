      "  type=\"${2|line,circle,dashboard|}\"",
      "  ${3:status=\"${4|normal,success,exception,active|}\"}",
      "  ${5::show-info=\"true\"}",
      "/>"
    ],
    "description": "Insert KPC Vue Progress component"
  },
  "KPC Vue Spin": {
    "prefix": "kpc-vue-spin",
    "body": [
      "<Spin",
      "  :spinning=\"${1:loading}\"",
      "  ${2:tip=\"${3:Loading...}\"}",
      "  ${4:size=\"${5|small,default,large|}\"}",
      ">",
      "  ${6:<!-- Content to be wrapped -->}",
      "</Spin>"
    ],
    "description": "Insert KPC Vue Spin (loading) component"
  },
  "KPC Vue Tabs": {
    "prefix": "kpc-vue-tabs",
    "body": [
