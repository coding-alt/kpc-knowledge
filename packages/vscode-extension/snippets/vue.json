      "const columns = [",
      "  {",
      "    title: '${4:Column Title}',",
      "    dataIndex: '${5:dataKey}',",
      "    key: '${5:dataKey}',",
      "  },",
      "  ${6:// Add more columns}",
      "];",
      "</script>"
    ],
    "description": "Insert KPC Vue Table with columns"
  },
  "KPC Vue Card": {
    "prefix": "kpc-vue-card",
    "body": [
      "<Card",
      "  title=\"${1:Card Title}\"",
      "  ${2::bordered=\"true\"}",
      "  ${3::hoverable=\"false\"}",
      ">",
      "  ${4:<!-- Card content -->}",
      "</Card>"
    ],
    "description": "Insert KPC Vue Card component"
  },
  "KPC Vue Menu": {
    "prefix": "kpc-vue-menu",
    "body": [
      "<Menu",
      "  mode=\"${1|horizontal,vertical,inline|}\"",
      "  :selected-keys=\"${2:selectedKeys}\"",
      "  @select=\"${3:handleSelect}\"",
      ">",
      "  <MenuItem key=\"${4:key1}\">",
      "    ${5:Menu Item 1}",
      "  </MenuItem>",
      "  <MenuItem key=\"${6:key2}\">",
      "    ${7:Menu Item 2}",
      "  </MenuItem>",
      "  ${8:<!-- Add more menu items -->}",
      "</Menu>"
    ],
