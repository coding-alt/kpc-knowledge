      "  @submit=\"${1:handleSubmit}\"",
      "  ${2:layout=\"vertical\"}",
      ">",
      "  <FormItem",
      "    label=\"${3:Label}\"",
      "    name=\"${4:fieldName}\"",
      "    ${5::required=\"true\"}",
      "  >",
      "    <Input v-model=\"${6:formData.${4:fieldName}}\" placeholder=\"${7:Enter value...}\" />",
      "  </FormItem>",
      "  ",
      "  <FormItem>",
      "    <Button type=\"submit\" variant=\"primary\">",
      "      ${8:Submit}",
      "    </Button>",
      "  </FormItem>",
      "</Form>"
    ],
    "description": "Insert KPC Vue Form with form items"
  },
  "KPC Vue Table": {
    "prefix": "kpc-vue-table",
    "body": [
      "<Table",
      "  :columns=\"columns\"",
      "  :data-source=\"${1:tableData}\"",
      "  ${2::pagination=\"true\"}",
      "  ${3::loading=\"false\"}",
      "/>",
      "",
