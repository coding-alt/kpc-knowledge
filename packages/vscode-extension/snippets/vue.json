      "  <Button>",
      "    ${3:Dropdown Button}",
      "  </Button>",
      "  <template #overlay>",
      "    <Menu @click=\"${4:handleMenuClick}\">",
      "      <MenuItem key=\"${5:key1}\">${6:Option 1}</MenuItem>",
      "      <MenuItem key=\"${7:key2}\">${8:Option 2}</MenuItem>",
      "    </Menu>",
      "  </template>",
      "</Dropdown>"
    ],
    "description": "Insert KPC Vue Dropdown component"
  },
  "KPC Vue Tooltip": {
    "prefix": "kpc-vue-tooltip",
    "body": [
      "<Tooltip",
      "  title=\"${1:Tooltip content}\"",
      "  placement=\"${2|top,bottom,left,right|}\"",
      "  ${3:trigger=\"hover\"}",
      ">",
      "  ${4:<!-- Wrapped content -->}",
      "</Tooltip>"
    ],
    "description": "Insert KPC Vue Tooltip component"
  },
  "KPC Vue Alert": {
    "prefix": "kpc-vue-alert",
    "body": [
