      "  v-model:active-key=\"${1:activeKey}\"",
      "  @change=\"${2:handleTabChange}\"",
      "  type=\"${3|line,card,editable-card|}\"",
      ">",
      "  <TabPane tab=\"${4:Tab 1}\" key=\"${5:tab1}\">",
      "    ${6:<!-- Tab 1 content -->}",
      "  </TabPane>",
      "  <TabPane tab=\"${7:Tab 2}\" key=\"${8:tab2}\">",
      "    ${9:<!-- Tab 2 content -->}",
      "  </TabPane>",
      "</Tabs>"
    ],
    "description": "Insert KPC Vue Tabs component"
  },
  "KPC Vue Steps": {
    "prefix": "kpc-vue-steps",
    "body": [
      "<Steps :current=\"${1:currentStep}\" direction=\"${2|horizontal,vertical|}\">",
      "  <Step title=\"${3:Step 1}\" description=\"${4:Step 1 description}\" />",
      "  <Step title=\"${5:Step 2}\" description=\"${6:Step 2 description}\" />",
      "  <Step title=\"${7:Step 3}\" description=\"${8:Step 3 description}\" />",
      "</Steps>"
    ],
    "description": "Insert KPC Vue Steps component"
  },
  "KPC Vue DatePicker": {
    "prefix": "kpc-vue-datepicker",
    "body": [
      "<DatePicker",
      "  v-model=\"${1:selectedDate}\"",
      "  @change=\"${2:handleDateChange}\"",
      "  placeholder=\"${3:Select date}\"",
      "  ${4:format=\"YYYY-MM-DD\"}",
      "  ${5::show-time=\"false\"}",
      "/>"
    ],
    "description": "Insert KPC Vue DatePicker component"
  },
  "KPC Vue Upload": {
    "prefix": "kpc-vue-upload",
    "body": [
      "<Upload",
      "  action=\"${1:/api/upload}\"",
      "  @change=\"${2:handleUploadChange}\"",
      "  ${3::multiple=\"false\"}",
